<h1>Login </h1>

<%= form_tag("/users/sessions") do %>
    <%= label_tag :email %>
    <%= text_field_tag :email %><br>
    <%= label_tag :password %>
    <%= password_field_tag :password %>
    <%= submit_tag "Login"%>
    <%  flash.each do |name, msg| %>
        <%= content_tag(:p, "#{msg}") %>
    <% end %>

<% end %>
<button><a href="/register">Sign Up</a></button> 